// font-family: 'Roboto', sans-serif;
// font-family: 'Amatic SC', cursive;
body
    margin 0
    padding 0
    box-sizing: border-box
    font-family: 'Roboto', sans-serif;
    font-size: 18px

h1, h2, h3, h4, h5, h6
    font-family: 'Amatic SC', cursive;

.dominicaduque
    background-image: url('../img/2019-11-10 21.08.58 2174479171430882837_211054777.jpg')
    // background-image: url('../img/2019-11-06 19.16.02 2171523227793137015_211054777.jpg')
    background-size cover
    background-position top right
    background-repeat no-repeat
    height 100vh
    display flex
    color #fff
    .data
        margin 0
        padding 0
        position relative
        top 50%
        left 5%
        h1
            font-size: 2.5em
        h2
            font-size: 1.5em
.main
    display inline-block
    // flex-direction row
    // flex-wrap: wrap
.reel
    position absolute
    top calc(55% + 150px)
    height 280px
    min-width 280px
    right 1em
    margin 1em
    background-color red
